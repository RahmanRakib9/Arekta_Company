
generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  email String  @unique
  name  String?
}

enum Role {
  USER
  ADMIN
  FACULTY
  MEMBER
}

enum AuthType {
  EMAILPASS
  OAUTH
}

